<%= form_for(resource, as: resource_name, url: password_path(resource_name), html: { method: :post,role: "login" }) do |f| %>
  <img src="/templogofeedback.png" class="img-responsive" alt="" />

  <%= devise_error_messages! %>

  <div class="field">
    <%= f.email_field :email, autofocus: true,:class => 'form-control input-lg',:placeholder => "Email" %>
  </div>

  <div class="actions">
    <%= f.submit "Send me reset password instructions", :class => 'btn btn-lg btn-primary btn-block' %>
  </div>
<% end %>